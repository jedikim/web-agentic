#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jedi/code/web-agentic/node-runtime/node_modules/.pnpm/patchright-core@1.57.0/node_modules/patchright-core/node_modules:/home/jedi/code/web-agentic/node-runtime/node_modules/.pnpm/patchright-core@1.57.0/node_modules:/home/jedi/code/web-agentic/node-runtime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jedi/code/web-agentic/node-runtime/node_modules/.pnpm/patchright-core@1.57.0/node_modules/patchright-core/node_modules:/home/jedi/code/web-agentic/node-runtime/node_modules/.pnpm/patchright-core@1.57.0/node_modules:/home/jedi/code/web-agentic/node-runtime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../patchright-core/cli.js" "$@"
else
  exec node  "$basedir/../../../../patchright-core/cli.js" "$@"
fi
